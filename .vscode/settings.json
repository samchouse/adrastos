{
  "rust-analyzer.check.command": "clippy",
  "eslint.workingDirectories": ["packages/*"],
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": [
    "Adrastos",
    "chrono",
    "deadpool",
    "dotenvy",
    "GETDEL",
    "Iden",
    "jsonwebtoken",
    "libinjection",
    "openapi",
    "pkce",
    "reqwest",
    "SETEX",
    "sqli",
    "totp",
    "utoipa"
  ],
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"],
    ["(?:cn|clsx)\\(([^\\);]*)[\\);]", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ]
}
