  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react",
    "outDir": "./out",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["./**/*.ts", "./**/*.tsx"],
}
